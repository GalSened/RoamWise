{
  "name": "traveling-app",
  "version": "2.0.0",
  "description": "AI-powered travel planning app with weather-aware routing",
  "type": "module",
  "scripts": {
    "start": "serve dist -l $PORT",
    "dev": "vite --config vite.simple.config.js",
    "build": "vite build --config vite.simple.config.js",
    "build:complex": "tsc && vite build",
    "build:simple": "vite build --config vite.simple.config.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "typecheck": "tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --passWithNoTests",
    "test:e2e": "npx playwright test",
    "test:e2e:local": "BASE_URL=http://localhost:5173/roamwise-app/ npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "e2e": "npm run test:e2e",
    "e2e:ui": "npm run test:e2e:ui",
    "ci": "npm run build",
    "diagnose": "npx tsx scripts/diagnose.ts"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.8",
    "comlink": "^4.4.1",
    "groq-sdk": "^0.37.0",
    "idb": "^8.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.468.0",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "serve": "^14.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-leaflet": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^27.4.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
